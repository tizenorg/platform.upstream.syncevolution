%.xml: %-full.xml
	$(XSLT) -o $@ $(srcdir)/spec-strip-docs.xsl $<

@COND_DOC_TRUE@%-doc.xml: %-full.xml
@COND_DOC_TRUE@	$(XSLT) -o $@ $(srcdir)/spec-to-docbook.xsl $<

@COND_DOC_TRUE@syncevo-dbus-api-doc.xml: syncevo-server-doc.xml syncevo-session-doc.xml syncevo-connection-doc.xml
@COND_DOC_TRUE@	echo '<?xml version="1.0" encoding="UTF-8"?>' >$@
@COND_DOC_TRUE@	echo '<reference><title>SyncEvolution D-Bus API $(VERSION)</title>' >>$@
@COND_DOC_TRUE@	for xml in $+; do \
@COND_DOC_TRUE@		tail -n +2 $$xml >>$@; \
@COND_DOC_TRUE@	done
@COND_DOC_TRUE@	echo '</reference>' >>$@

@COND_DOC_TRUE@syncevo-dbus-api-doc.html: syncevo-dbus-api-doc.xml
@COND_DOC_TRUE@	$(XSLT) -o $@ http://docbook.sourceforge.net/release/xsl/current/html/docbook.xsl $<

@COND_GUI_TRUE@%-bindings.h: stamp-%-bindings.h
@COND_GUI_TRUE@	@true
@COND_GUI_TRUE@stamp-%-bindings.h: %.xml
@COND_GUI_TRUE@	$(DBUS_BINDING_TOOL) --mode=glib-client --prefix=syncevo $< > xgen-$(@F) \
@COND_GUI_TRUE@	&& (cmp -s xgen-$(@F) $(@F:stamp-%=%) || cp xgen-$(@F) $(@F:stamp-%=%)) \
@COND_GUI_TRUE@	&& rm -f xgen-$(@F) \
@COND_GUI_TRUE@	&& echo timestamp > $(@F)

@COND_GUI_TRUE@syncevo-marshal.h: $(srcdir)/syncevo-marshal.list $(GLIB_GENMARSHAL)
@COND_GUI_TRUE@	$(GLIB_GENMARSHAL) $< --header --prefix=syncevo_marshal > $@
@COND_GUI_TRUE@syncevo-marshal.c: $(srcdir)/syncevo-marshal.list syncevo-marshal.h $(GLIB_GENMARSHAL)
@COND_GUI_TRUE@	echo "#include \"syncevo-marshal.h\"" > $@ \
@COND_GUI_TRUE@	&& $(GLIB_GENMARSHAL) --prefix=syncevo_marshal $(srcdir)/syncevo-marshal.list --body >> $@

@COND_GUI_TRUE@%-glue.h: stamp-%-glue.h
@COND_GUI_TRUE@	@true
@COND_GUI_TRUE@stamp-%-glue.h: %.xml
@COND_GUI_TRUE@	$(DBUS_BINDING_TOOL) --prefix=syncevo --mode=glib-server $< > xgen-$(@F) \
@COND_GUI_TRUE@	&& (cmp -s xgen-$(@F) $(@F:stamp-%=%) || cp xgen-$(@F) $(@F:stamp-%=%)) \
@COND_GUI_TRUE@	&& rm -f xgen-$(@F) \
@COND_GUI_TRUE@	&& echo timestamp > $(@F)
