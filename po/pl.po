msgid "Not authorized"
msgstr "Brak autoryzacji"

#: ../src/gtk-ui/sync-ui.c:1935
msgid "Forbidden"
msgstr "Nie dozwolone"

# has not been found
#: ../src/gtk-ui/sync-ui.c:1937
msgid "Not found"
