msgstr "Nieprawidłowy URL"

#: ../src/gtk-ui/sync-ui.c:1973
msgid "Server not found"
msgstr "Nie znaleziono serwera"

#: ../src/gtk-ui/sync-ui.c:1975
#, c-format
msgid "Error %d"
msgstr "Błąd %d"

