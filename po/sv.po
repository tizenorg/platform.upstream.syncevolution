#: ../src/gtk-ui/sync-ui.c:816
#, c-format
msgid "Last synced %ld days ago"
msgstr "Senaste synk %ld dagar sedan"

#: ../src/gtk-ui/sync-ui.c:901
msgid "Sync again"
msgstr "Synkronisera igen"

