msgstr "Sincronização"

#: ../src/gtk-ui/sync-ui.c:259
msgid "Addressbook"
msgstr "Endereços"

#: ../src/gtk-ui/sync-ui.c:261
msgid "Calendar"
msgstr "Calendário"

#: ../src/gtk-ui/sync-ui.c:263
msgid "Todo"
msgstr "Tarefas"

#: ../src/gtk-ui/sync-ui.c:265
msgid "Memo"
